<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk+" version="3.18"/>
    <requires lib="gtkwidgets" version="0.0"/>
    <object class="GtkBox" id="check">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="orientation">vertical</property>
        <property name="margin_top">40</property>
        <property name="halign">center</property>
        <child>
            <object class="GtkLabel" id="prepare_best_results">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="xalign">0</property>
                <property name="label" translatable="yes">&lt;span weight="bold" size="large"&gt;For best results, please ensure that this computer:&lt;/span&gt;</property>
                <property name="use_markup">True</property>
                <property name="margin_bottom">14</property>
            </object>
            <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
            </packing>
        </child>
        <child>
            <object class="GtkBox" id="vbox1">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="orientation">vertical</property>
                <property name="spacing">7</property>
                <property name="margin_bottom">10</property>
                <property name="hexpand">True</property>
                <child>
                    <object class="StateBox" id="prepare_enough_space">
                        <property name="visible">True</property>
                        <property name="label">has at least 3GB available drive space</property>
                    </object>
                    <packing>
                        <property name="expand">True</property>
                        <property name="fill">True</property>
                        <property name="position">0</property>
                    </packing>
                </child>
                <child>
                    <object class="StateBox" id="prepare_power_source">
                        <property name="visible">True</property>
                        <property name="label">is plugged in to a power source</property>
                    </object>
                    <packing>
                        <property name="expand">True</property>
                        <property name="fill">True</property>
                        <property name="position">1</property>
                    </packing>
                </child>
                <child>
                    <object class="StateBox" id="prepare_network_connection">
                        <property name="visible">True</property>
                        <property name="label">is connected to the Internet</property>
                    </object>
                    <packing>
                        <property name="expand">True</property>
                        <property name="fill">True</property>
                        <property name="position">2</property>
                    </packing>
                </child>
                <child>
                    <object class="StateBox" id="updated">
                        <property name="visible">True</property>
                        <property name="label">Cnchi is up to date</property>
                    </object>
                    <packing>
                        <property name="expand">True</property>
                        <property name="fill">True</property>
                        <property name="position">3</property>
                    </packing>
                </child>
                <child>
                    <object class="StateBox" id="packaging_issues">
                        <property name="visible">True</property>
                        <property name="label">There are no packaging issues that would interfere with installation.</property>
                    </object>
                    <packing>
                        <property name="expand">True</property>
                        <property name="fill">True</property>
                        <property name="position">4</property>
                    </packing>
                </child>
            </object>
            <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">1</property>
            </packing>
        </child>
        <child>
            <object class="GtkBox" id="third_party_vbox">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="orientation">vertical</property>
                <property name="spacing">10</property>
                <child>
                    <object class="GtkLabel" id="label_space">
                        <property name="visible">True</property>
                        <property name="can_focus">False</property>
                        <property name="xalign">0</property>
                        <property name="label" translatable="yes">(*) This highly depends on which desktop environment you choose, so you might need more space (for instance, KDE needs 6.5GB).</property>
                        <property name="use_markup">True</property>
                        <property name="wrap">True</property>
                    </object>
                    <packing>
                        <property name="expand">False</property>
                        <property name="fill">True</property>
                        <property name="position">0</property>
                    </packing>
                </child>
                <!--
        <child>
          <object class="GtkImage" id="image1">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">1</property>
          </packing>
        </child>
        -->
            </object>
            <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">2</property>
            </packing>
        </child>
    </object>
</interface>
